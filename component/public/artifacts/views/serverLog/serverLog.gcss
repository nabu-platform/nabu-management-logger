[route=serverLog] {
	padding: 20px 0px;
	
	.buttons {
		padding-bottom: 10px;
		text-align: center;
	}
	.log {
		overflow: auto;
		width: 100%;
		font-family: Monospace;
		font-size: 9pt;
		
		.logged, .errorCode, .log-message {
			margin: 1rem 0px;
		}
		
		.logged {
			color: #888;
			border: solid 1px #ccc;
			border-style: none solid none none;
			margin-right: 10px;
			padding: 0 1rem;
		}
		pre {
			margin: 0;
		}
	}
}